{
  "manifest_version": 2,
"name": "__MSG_appName__",
"description": "__MSG_appDesc__",
"default_locale": "en",
  "version": "3.1.0",
 "icons": { "128": "icon.png" },
"content_scripts": [
    {
      "matches": ["http://*/","http://*/*","https://www.youtube.com/*","https://vimeo.com/*"],
      "all_frames": true,
      "js": ["background2.js"]
    }
  ],
"web_accessible_resources": [
"background.js",
"icon.png",
"options.html"
],
"permissions": [
"storage"
  ],
"options_page": "options.html"
}
